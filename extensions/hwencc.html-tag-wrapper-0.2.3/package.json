{
	"name": "html-tag-wrapper",
	"displayName": "html tag wrapper",
	"description": "wrap selected html tag by press ctrl+i, you can change the wrapper tag name simply too",
	"version": "0.2.3",
	"publisher": "hwencc",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/hwen/tag-wrapper"
	},
	"bugs": {
		"url": "https://github.com/hwen/tag-wrapper/issues"
	},
	"engines": {
		"vscode": "^1.10.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.wrapTag"
	],
	"main": "./out/src/extension",
	"contributes": {
		"keybindings": [
			{
				"key": "ctrl+i",
				"command": "extension.wrapTag",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"dependencies": {},
	"__metadata": {
		"installedTimestamp": 1739536747622,
		"size": 323715
	}
}